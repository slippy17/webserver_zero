<!DOCTYPE html>

<!-- https://www.gitto.tech/posts/create-simple-music-player-app-using-html-css-and-javascript/ -->

<html>
<head>
	<title> Pioneer Audio Player </title>
	<link rel="shortcut icon" type="image/jpg" href="./static/pioneer.png"/>
	<link rel='stylesheet' type="text/css" href="/static/styles.css">
	<link rel="stylesheet" href="static/font-awesome-4.7.0/css/font-awesome.min.css">
</head>


<body>

<div class="player">
	<div class="main">
		<audio></audio>
		<div class="thumbnail">
			<img src="">
		</div>
		<div class="seekbar">
			<input type="range">
			
		</div>

		<div class="details">
			<div class="container">
			<h5 id="runStat">00:00</h5>
			</div>
			<p></p>
			<h3></h3>
			<h2></h2>
		</div>
		<div class="controls">
			 <div class="prev-disc-control">
				<i class="fa fa-minus-circle"></i> 
			</div>
			<div class="prev-control">
				<i class="fa fa-backward"></i>
			</div>
			<div class="play-pause-control paused">
				<i class="fa fa-play"></i>
				<i class="fa fa-pause"></i>
				<i class="fa fa-play-circle-o"></i>
			</div>
			<div class="next-control">
				<i class="fa fa-forward"></i>
			</div>
			<div class="next-disc-control">
				<i class="fa fa-plus-circle"></i> 
			</div>

		<div class="search">
			<button>Search</button><
			</div>
			<div class="vol-up-control">
				<i class="fa fa-volume-up"></i> 
			</div>
			<div class="vol-down-control">
				<i class="fa fa-volume-down"></i> 
			</div>



		</div>
	</div>
	<div class="player-list">
		<div class="toggle-list">
			<i class="fa fa-angle-up"></i>
			<i class="fa fa-angle-down"></i>
		</div>
		<div class="list">
		</div>
	</div>
</div>

<script src="static/script.js"></script>
</body>
</html>